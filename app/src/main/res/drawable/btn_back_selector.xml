<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
  android:constantSize="false" android:dither="false">
    <item android:state_pressed="true"
          android:drawable="@drawable/back_press" /> <!-- pressed -->
    <item android:state_enabled="false"
          android:drawable="@drawable/back_press"/>
    <item android:state_focused="true"
        android:state_pressed="@drawable/back_press"
        android:drawable="@drawable/back_normal" />    
    <item android:drawable="@drawable/back_normal" />
    

</selector>
